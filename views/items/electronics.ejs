
  <%- include ../head.ejs %>
    <meta charset="utf-8">
    <title>Electronic Items</title>
  </head>
  <body>
    <header>
      <nav>
        <ul id='navmenu'>
          <a href="/featureditems">Home</a>
          <a href="/featureditems/new">Sell</a>
          <a href="/profile/<%=userId%>"><%=username%></a>
          <a href="/auth/logout">Logout</a>
        </ul>
      </nav>
    </header>
    <div class="container" class="text-center" class="form-group">
      </br>
    <h1>Electronic Items</h1>
    <hr>
    <ul class="list-unstyled">
      <% for (let i = 0; i < items.length; i++){%>
        <% if (items[i].category === 'electronics') { %>
           <li>
            <a href="/featureditems/<%=items[i].id%>">
              <h1><%=items[i].title %></h1>
             </a></li>
             <li><h3>$<%=items[i].price %></h3></li>
             <li><h3><%=items[i].description %></h3></li><br/>
           <img src="<%=items[i].image%>" style= width:225px;height300px>
           <hr></br>
       <% } %>
      <%}%>
    </ul>
    </div>
  </body>
</html>
